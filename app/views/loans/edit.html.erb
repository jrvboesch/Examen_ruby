<h1>Editar Prestamo</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
  <%= render partial: "partials/errors", locals: {obj: @loan} %>

    <%= form_for @loan do | f | %>
      <%= render "partials/loans_form", f: f %>
      <%= f.submit "Actualizar Prestamo", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>

    
